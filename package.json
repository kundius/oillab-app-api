    "@types/multer": "^1.4.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^9.0.8",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
