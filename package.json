    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/typeorm": "^10.0.2",
    "@types/wkhtmltopdf": "^0.3.9",
    "apollo-server-express": "^3.13.0",
    "argon2": "^0.40.1",
    "aws-sdk": "^2.1620.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "file-type": "^16.5.3",
    "graphql": "^16.8.1",
    "graphql-fields-to-relations": "^1.0.5",
